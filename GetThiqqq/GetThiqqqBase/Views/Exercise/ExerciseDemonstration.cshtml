@model ExerciseDemonstratonViewModel
@{
    ViewData["Title"] = Model.Exercise.ExerciseName;
    ViewData["Id"] = Model.UserId;
}

<h3>@Model.Exercise.ExerciseName</h3>

<p>@Model.Exercise.Description</p>

<p style="white-space: pre-line">@Model.Exercise.Instructions</p>

<iframe width="420" height="315"
src="@Model.Exercise.VideoLink" frameborder="0" allowfullscreen>
</iframe>